{
    "MpiEncoder": {
        "Packer": {
            "enableMerging": true,
            "enablePatchInPatch": true,
            "enablePatchInformation": false,
            "enableRecursiveSplit": false,
            "minPatchSize": 8,
            "overlap": 1,
            "sortingMethod": 1
        },
        "PackerMethod": "Packer",
        "textureDilation": 2,
        "transparencyDynamic": 4
    },
    "blockSizeDepthQualityDependent": [8, 8],
    "configDirectory": "config",
    "inputDirectory": ".",
    "inputMpiPcsPathFmt": "{1}/{3}.pcs",
    "inputSequenceConfigPathFmt": "ctc/sequences/{1}.json",
    "intraPeriod": 32,
    "outputBitstreamPathFmt": "M{0}/{1}/TMIV_M{0}_{1}.bit",
    "outputDirectory": ".",
    "outputTextureVideoDataPathFmt": "M{0}/{1}/TMIV_M{0}_{1}_tex_c{3:02}_{4}x{5}_{6}.yuv",
    "outputTransparencyVideoDataPathFmt": "M{0}/{1}/TMIV_M{0}_{1}_tra_c{3:02}_{4}x{5}_{6}.yuv",
    "overrideAtlasFrameSizes": [[4096, 4096]],
    "viewportCameraParametersSei": false,
    "viewportPositionSei": false
}
