#ifndef TMIV_VIDEODECODER_VERSIONS_H
#define TMIV_VIDEODECODER_VERSIONS_H

#include <string_view>
#include <vector>

namespace TMIV::VideoDecoder {
static constexpr bool found_hm = 0 < @HM_FOUND@;
static constexpr bool found_vvdec = 0 < @vvdec_FOUND@;
static constexpr std::string_view hm_version = "HEVC Model (HM) @HM_VERSION@";
static constexpr std::string_view vvdec_version = "Fraunhofer VVdeC @vvdec_VERSION@";

[[nodiscard]] inline auto versions() -> std::vector<std::string_view> {
  auto result = std::vector<std::string_view>{};

  if constexpr (found_hm) {
    result.push_back(hm_version);
  }
  if constexpr (found_vvdec) {
    result.push_back(vvdec_version);
  }
  return result;
}
} // namespace TMIV::VideoDecoder

#endif // TMIV_VIDEODECODER_VERSIONS_H
